<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">

  <head>

    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <%= stylesheet_link_tag "/theme/styles/style_new" %>
<script type="text/javascript" src="/scripts/home_random_quotes.js"></script>

    <title><%= page_title %></title>

    <%= yield :html_head %>

  </head>

  <body>

    <%= cms_toolbar %>

 <%= render :partial => "partials/header"%>

<div class="container ruleoutside">
  <%= render :partial => "partials/left_nav"%>

	<div class="span-12 last">

		<div id="title">
		<h1>
          <%= container :title %>
        </h1>

		</div><!-- title ends -->

	<div class="textcontainer rule4">

		<div class="span-3">
		<div class="text">
          <h3 class="sans"><%= container :head_left %></h3>
          <p class="sans"><%= container :subhead_left %></p>
          <p class="sans"><%= container :content_left %></p>
		</div><!-- text ends -->
		</div><!-- left column ends -->

		<div class="span-3">
		<div class="text">
          <h3 class="sans"><%= container :head_left1 %></h3>
          <p class="sans"><%= container :subhead_left1 %></p>
          <p class="sans"><%= container :content_left1 %></p>
		</div><!-- text ends -->
		</div><!-- middle column ends -->

		<div class="span-3">
		<div class="text">
          <h3 class="sans"><%= container :head_right %></h3>
          <p class="sans"><%= container :subhead_right %></p>
          <p class="sans"><%= container :content_right %></p>
		</div><!-- text ends -->
		</div><!-- right column ends -->

		<div class="span-3 last">
		<div class="text">
          <h3 class="sans"><%= container :head_right1 %></h3>
          <p class="sans"><%= container :subhead_right1 %></p>
          <p class="sans"><%= container :content_right1 %></p>
		</div><!-- text ends -->
		</div><!-- right column ends -->

	</div> <!-- textcontainer ends -->

	</div> <!-- span-12 last ends -->

	</div> <!-- container ends -->

<%= render :partial => "partials/footer"%>



  </body>

</html>
